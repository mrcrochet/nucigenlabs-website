{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "build": "npx vite build",
    "lint": "npx eslint .",
    "preview": "npx vite preview",
    "typecheck": "npx tsc --noEmit -p tsconfig.app.json",
    "check-env": "node check-env.js",
    "phase1:insert-test": "npx tsx src/server/phase1/insert-test-events.ts",
    "phase1:validate": "npx tsx src/server/phase1/phase1_validate.ts",
    "phase2b:validate": "npx tsx src/server/phase2b/phase2b_validate.ts",
    "pipeline:collect": "npx tsx src/server/workers/data-collector.ts",
    "pipeline:process": "npx tsx src/server/workers/event-processor.ts",
    "pipeline:run": "npx tsx src/server/workers/pipeline-orchestrator.ts",
    "pipeline:run-once": "RUN_ONCE=true npx tsx src/server/workers/pipeline-orchestrator.ts",
    "quality:metrics": "npx tsx src/server/phase3b/quality-metrics-calculator.ts",
    "quality:metrics-range": "npx tsx src/server/phase3b/quality-metrics-calculator.ts range",
    "alerts:generate": "npx tsx src/server/workers/alerts-generator.ts",
    "alerts:generate:debug": "DEBUG=true npx tsx src/server/workers/alerts-generator.ts",
    "alerts:init": "npx tsx src/server/phase3c/init-alert-preferences.ts"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.57.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "lucide-react": "^0.344.0",
    "openai": "^4.104.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.10.0",
    "tailwind-merge": "^3.4.0",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.21"
  }
}
